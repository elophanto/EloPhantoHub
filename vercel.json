{
  "rewrites": [
    {
      "source": "/v1/:path*",
      "has": [{ "type": "host", "value": "api.elophanto.com" }],
      "destination": "/api/:path*"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/push-dataset",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/refresh-stats",
      "schedule": "0 * * * *"
    }
  ]
}
